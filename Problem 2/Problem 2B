a = float(input())
b = float(input())
bmi= (a/(b/100)**2)
print(round(bmi,2))
if bmi<18.5:
    print("Below normal weight")
elif 18.5<=bmi<25:
    print("Normal weight")
elif 25<=bmi<30:
    print("Overweight")
elif 30<=bmi<35:
    print("Class I Obesity")
elif 35<=bmi<40:
    print("Class II Obesity")
elif bmi>=40:
    ("print Class III Obesity")